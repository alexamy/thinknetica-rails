<%= form_with(model: [:admin, @badge], local: true) do |form| %>
  <%= render 'shared/errors', resource: @badge %>

  <div class="form-group">
    <%= form.label :filename %>
    <%= form.select :filename, badges_for_select, {}, { class: 'form-control' } %>
  </div>

  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
  </div>

  <div class="form-group">
  </div>

  <div class="form-group">
  </div>
<% end %>
