<tr>
  <td>
    <%= test.title %>
    <%= render 'form_inline', test: test %>
  </td>
  <td><%= test.level %></td>
  <td><%= test.questions.size %></td>
  <td><%= link_to t('h.show'), admin_test_path(test) %></td>
  <td><%= link_to t('h.edit'), edit_admin_test_path(test) %></td>
  <td><%= link_to t('h.delete'),
                  admin_test_path(test),
                  method: :delete,
                  data: { confirm: t('h.are_you_sure') } %></td>
</tr>
