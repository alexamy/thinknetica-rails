<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.size %></td>
  <td><%= link_to 'Показать', test_path(test) %></td>
  <td><%= link_to 'Редактировать', edit_test_path(test) %></td>
  <td><%= button_to 'Начать', start_test_path(test) %></td>
  <td><%= link_to 'Удалить',
                  test_path(test),
                  method: :delete,
                  data: { confirm: 'Вы уверены?' } %></td>
</tr>
